#include <iostream>
using namespace std;

int main() {
    int arr[] = {-2,-5,6,-2,-3,1,5,-6};
    int n = 8;

    int maxSum = arr[0];
    int currSum = arr[0];

    for (int i = 1; i < n; i++) {
        currSum = max(arr[i], currSum + arr[i]);
        maxSum = max(maxSum, currSum);
    }

    cout << maxSum;
    return 0;
}
